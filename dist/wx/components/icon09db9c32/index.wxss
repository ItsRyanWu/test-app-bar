@import "../../styles/uno.wxss";
.icon__inner {
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
  --home: url(data:image/svg+xml;base64,);
  --chart: url(data:image/svg+xml;base64,);
  --badge: url(data:image/svg+xml;base64,);
  --report: url(data:image/svg+xml;base64,);
}
.icon__inner.fill-contain {
  background-size: contain;
}
.icon__inner.fill-cover {
  background-size: cover;
}
.icon__inner.icon-home.non-colorable {
  background-image: var(--home);
}
.icon__inner.icon-home.colorable {
  -webkit-transform: scale(1.1);
          transform: scale(1.1);
  -webkit-transform-origin: 50% 50%;
          transform-origin: 50% 50%;
  -webkit-mask-image: var(--home);
          mask-image: var(--home);
  transition: background-color 0.3s;
}
.icon__inner.icon-chart.non-colorable {
  background-image: var(--chart);
}
.icon__inner.icon-chart.colorable {
  -webkit-transform: scale(1.1);
          transform: scale(1.1);
  -webkit-transform-origin: 50% 50%;
          transform-origin: 50% 50%;
  -webkit-mask-image: var(--chart);
          mask-image: var(--chart);
  transition: background-color 0.3s;
}
.icon__inner.icon-badge.non-colorable {
  background-image: var(--badge);
}
.icon__inner.icon-badge.colorable {
  -webkit-transform: scale(1.1);
          transform: scale(1.1);
  -webkit-transform-origin: 50% 50%;
          transform-origin: 50% 50%;
  -webkit-mask-image: var(--badge);
          mask-image: var(--badge);
  transition: background-color 0.3s;
}
.icon__inner.icon-report.non-colorable {
  background-image: var(--report);
}
.icon__inner.icon-report.colorable {
  -webkit-transform: scale(1.1);
          transform: scale(1.1);
  -webkit-transform-origin: 50% 50%;
          transform-origin: 50% 50%;
  -webkit-mask-image: var(--report);
          mask-image: var(--report);
  transition: background-color 0.3s;
}